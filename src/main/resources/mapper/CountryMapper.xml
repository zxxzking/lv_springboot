<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zxxz.lv.mapper.CountryMapper">
    <resultMap id="country" type="com.zxxz.lv.entity.Country">
        <id column="Id" property="id" ></id>
        <result property="countryname" column="countryname"></result>
        <result property="countrycode" column="countrycode"></result>
    </resultMap>

    <select id="getById" resultMap="country" parameterType="int" resultType="com.zxxz.lv.entity.Country">
        select * from country where Id = #{id}
    </select>
</mapper>